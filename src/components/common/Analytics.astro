---
import GoogleAnalytics from '../common/GoogleAnalytics.astro';
import GoogleTagManager from './GoogleTagManager.astro';
import FacebookPixel from '../common/FacebookPixel.astro';
import { ANALYTICS } from 'astrowind:config';
---

{ANALYTICS?.vendors?.googleAnalytics?.id ? <GoogleAnalytics id={String(ANALYTICS.vendors.googleAnalytics.id)} /> : null}
{ANALYTICS?.vendors?.googleTagManager?.id ? <GoogleTagManager id={String(ANALYTICS.vendors.googleTagManager.id)} /> : null}
{ANALYTICS?.vendors?.facebookPixel?.id ? <FacebookPixel id={String(ANALYTICS.vendors.facebookPixel.id)} /> : null}
